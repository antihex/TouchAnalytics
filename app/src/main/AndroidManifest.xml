<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.touchanalytics">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.TouchAnalytics">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OpenSaveCSV"></activity>
        <activity android:name=".CollectSwipe"></activity>
        <activity android:name=".CalibrateUser"></activity>
        <activity android:name=".TestSwipe"></activity>
        <activity android:name=".IntruderPage"></activity>
        <activity android:name=".TrueUserPage"></activity>
        <activity android:name=".DeleteUser"></activity>
        <activity android:name=".Debug"></activity>
    </application>

</manifest>